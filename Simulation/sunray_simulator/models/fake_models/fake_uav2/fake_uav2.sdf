    <link name='rotor_3'>
			<pose>-0.065 -0.065 -0.025 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.005</mass>
        <inertia>
          <ixx>9.75e-07</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000173104</iyy>
          <iyz>0</iyz>
          <izz>0.000174004</izz>
        </inertia>
      </inertial>
      <visual name='rotor_3_visual'>
        <pose>0 0 0 1.57 0 0</pose>
        <geometry>
          <mesh>
            <scale>0.03 0.03 0.03</scale>
            <uri>model://fake_uav2/meshes/sunray_cw.stl</uri>
