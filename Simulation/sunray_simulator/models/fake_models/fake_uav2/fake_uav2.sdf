<?xml version='1.0'?>
<sdf version='1.6'>
  <model name='fake_uav2'>
  <static>1</static>
    <!-- sunray body -->
    <link name='base_link'>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.0085</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0085</iyy>
          <iyz>0</iyz>
          <izz>0.012</izz>
        </inertia>
      </inertial>
      <visual name='base_link_inertia_visual'>
        <pose>0 0 0 0 0 -1.57</pose>
        <geometry>
          <mesh>
            <scale>0.03 0.03 0.03</scale>
            <uri>model://fake_uav2/meshes/sunray.stl</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/DarkGrey</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </link>
    <link name='rotor_0'>
			<pose>0.065 -0.065 -0.025 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.005</mass>
        <inertia>
          <ixx>9.75e-07</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000173104</iyy>
          <iyz>0</iyz>
          <izz>0.000174004</izz>
        </inertia>
      </inertial>
      <visual name='rotor_0_visual'>
        <pose>0 0 0 1.57 0 0</pose>
        <geometry>
          <mesh>
            <scale>0.03 0.03 0.03</scale>
            <uri>model://fake_uav2/meshes/sunray_cw.stl</uri>
