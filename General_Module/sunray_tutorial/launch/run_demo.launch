<launch>
	<arg name="uav_id" default="1"/>
    <arg name="uav_name" default="uav" />
    <arg name="demo_id" default="1" />
    
	<node if="$(eval demo_id == 1)" pkg="sunray_tutorial" type="takeoff_hover_land" name="takeoff_hover_land$(arg uav_id)" output="screen">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<node if="$(eval demo_id == 2)" pkg="sunray_tutorial" type="block_pos" name="block_pos$(arg uav_id)" output="screen">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<node if="$(eval demo_id == 3)" pkg="sunray_tutorial" type="circle_vel" name="circle_vel$(arg uav_id)" output="screen">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<node if="$(eval demo_id == 4)" pkg="sunray_tutorial" type="circle_z_pos" name="circle_z_pos$(arg uav_id)" output="screen">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<node if="$(eval demo_id == 5)" pkg="sunray_tutorial" type="pos_body_hexagon" name="pos_body_hexagon$(arg uav_id)" output="screen">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<!-- 新增python脚本节点 -->
	<node if="$(eval demo_id == 11)" pkg="sunray_tutorial" type="uav_python/takeoff_hover_land.py" name="takeoff_hover_land_py$(arg uav_id)" output="screen" required="true">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<node if="$(eval demo_id == 12)" pkg="sunray_tutorial" type="uav_python/block_pos.py" name="block_pos_py$(arg uav_id)" output="screen" required="true">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<node if="$(eval demo_id == 13)" pkg="sunray_tutorial" type="uav_python/circle_vel.py" name="circle_vel_py$(arg uav_id)" output="screen" required="true">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<node if="$(eval demo_id == 14)" pkg="sunray_tutorial" type="uav_python/circle_z_pos.py" name="circle_z_pos_py$(arg uav_id)" output="screen" required="true">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<node if="$(eval demo_id == 15)" pkg="sunray_tutorial" type="uav_python/pos_body_hexagon.py" name="pos_body_hexagon_py$(arg uav_id)" output="screen" required="true">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>
	
	<!-- 其他原有节点保持不变 -->
	<node if="$(eval demo_id == 6)" pkg="sunray_tutorial" type="vel_body_follow_car" name="vel_body_follow_car$(arg uav_id)" output="screen">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<node if="$(eval demo_id == 7)" pkg="sunray_tutorial" type="vel_body_z_pos_follow_car" name="vel_body_z_pos_follow_car$(arg uav_id)" output="screen">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node>

	<!-- <node pkg="sunray_tutorial" type="follow_a_car" name="follow_a_car_$(arg uav_id)" output="screen">
		<param name="uav_id" value="$(arg uav_id)" />
		<param name="uav_name" value="$(arg uav_name)" />
	</node> -->
</launch>

